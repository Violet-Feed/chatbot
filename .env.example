# --- App ---
APP_ENV=dev
LOG_LEVEL=INFO

# --- RocketMQ ---
ROCKETMQ_NAME_SERVER=127.0.0.1:9876
ROCKETMQ_TOPIC=im_conv
ROCKETMQ_CONSUMER_GROUP=chatbot_conv_consumer
ROCKETMQ_SUBSCRIPTION_EXPRESSION=*

# --- Redis / MySQL ---
REDIS_URL=redis://127.0.0.1:6379/0
MYSQL_DSN=mysql+asyncmy://root:root@127.0.0.1:3306/violet?charset=utf8mb4

# --- IMService gRPC ---
IM_GRPC_TARGET=127.0.0.1:3004

# --- Window policy ---
WINDOW_BASE_MS=5000
WINDOW_MAX_MS=10000

# --- Two-bot gap policy ---
SECOND_GAP_MIN_MS=3000
SECOND_GAP_MAX_MS=8000

# --- Group rate limit (token bucket) ---
GROUP_BUCKET_CAP=3
GROUP_BUCKET_PERIOD_MS=300000

# --- bot↔bot strict token ---
BOT2BOT_TOKEN_TTL_SEC=600

# --- Scheduler worker ---
SCHEDULER_POLL_INTERVAL_MS=200
SCHEDULER_BATCH_SIZE=64

# DashScope API Key（OpenAI-compatible 同样用它做 Bearer）
DASHSCOPE_API_KEY=你的key

# 选北京 or 新加坡（按你的key所属地域）
QWEN_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
# QWEN_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1

QWEN_MODEL=qwen-plus
QWEN_TEMPERATURE=0.7
QWEN_TIMEOUT_SEC=30

# Jina Search
JINA_API_KEY=你的key
JINA_SEARCH_BASE_URL=https://s.jina.ai/http://www.google.com/search?q=
JINA_SEARCH_TIMEOUT_SEC=20

